{
  "name": "contributte/forms",
  "description": "Extra contrib to nette/forms",
  "keywords": [
    "nette",
    "forms",
    "inputs",
    "checkbox",
    "rules",
    "validation",
    "renderers"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/forms",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.2",
    "nette/forms": "^3.2.5"
  },
  "require-dev": {
    "contributte/qa": "^0.4.0",
    "contributte/phpstan": "^0.3.0",
    "contributte/tester": "^0.3.0",
    "nette/application": "^3.0.0",
    "nette/di": "^3.1.0"
  },
  "suggest": {
    "nette/di": "to use FormFactoryExtension[CompilerExtension]"
  },
  "conflict": {
    "nette/di": "<3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Forms\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "0.6.x-dev"
    }
  }
}
